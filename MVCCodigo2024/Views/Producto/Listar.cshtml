
@{
    ViewBag.Title = "Listar";
}
@model List<MVCCodigo2024.Models.Producto>
<h2>Listar</h2>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Cantidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var producto in Model)
            {
                <tr>
                    <td>@producto.Id</td>
                    <td>@producto.Nombre</td>
                    <td>@producto.Precio</td>
                    <td>@producto.Cantidad</td>
                </tr>
            }
    </tbody>
</table>

